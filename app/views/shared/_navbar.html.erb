<nav>
    <% if user_signed_in? %>
        <%= link_to "Students", students_url %>
        <%= link_to "Performance Tests", performance_tests_url %>
        <%= link_to "Test Types", test_types_url %>
        <%= form_tag(destroy_user_session_path, method: :delete, style: 'display:inline-block;') do %>
          <% submit_tag "Sign Out" %>
        <% end %>
    <% else %>
        <%= link_to "Sign Up", new_user_registration_url %>
        <%= link_to "Sign In", user_session_url %>
    <% end %>
</nav>